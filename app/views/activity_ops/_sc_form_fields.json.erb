[
	{"fieldLabel":"id","bind":"{rec.id}","disabled":true},
	{"fieldLabel":"Lavorazione","bind":"{rec.name}","maxLength":50},
	{"fieldLabel":"Descr. breve","bind":"{rec.short_name}","maxLength":20},
	{"fieldLabel":"Codice","bind":"{rec.code}","maxLength":10},
	<%= raw extjs_std_numberfield_bind('default_price', @new_rec, {allowBlank: true}) -%>,
	<%= raw extjs_std_combo_bind('activity_type', @new_rec, valueField: 'id', displayField: 'name', :store => extjs_std_store_data(ActivityType.all)) -%>,
	{"fieldLabel":"Messa a disposizione","bind":"{rec.to_be_made_available}", xtype: 'checkbox'},
	
	{"fieldLabel":"Codice gestionale","bind":"{rec.gest_code}","maxLength":50},
	{"fieldLabel":"Usa prezzo da listino in importazione su gestionale","bind":"{rec.recalculate_gest_price}", xtype: 'checkbox'},
	
	{"fieldLabel":"Escludi se gestione vuoti (su compagnia)","bind":"{rec.exclude_if_manage_empty}", xtype: 'checkbox'},
	{"fieldLabel":"Escludi se gestione pieni (su compagnia)","bind":"{rec.exclude_if_manage_full}", xtype: 'checkbox'},
	
]
