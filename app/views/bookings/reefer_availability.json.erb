{
  success: true,
  items: [
		 {
				flex: 1,
				title: 'Frigo',
				closable: true,
				layout: 'fit',
        	    tools: [
                ],
        	    xtype: 'gridpanel',
				autoScroll: true,  
				scroll: true,
				
			    columns: [
			       {dataIndex: 'shipowner_name',    text: 'Compagnia',       flex:  1},
