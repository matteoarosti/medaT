{
  xtype: 'sc_grid',
  model_name: <%= raw (sc_model_class).to_json -%>,
  
  filter_url: <%= raw url_for(:action => 'extjs_sc_crt_filter_view').to_json -%>,
  
  title: <%= raw (sc_model_class + ' List').to_json -%>,
  closable: true,
  
  columns: <%= render :partial=>"sc_grid_columns" -%>,
  form_fields: <%= render :partial=>"sc_form_fields" -%>,
      
  viewModel: {
  	stores:   {customers: <%= raw(sc_store.to_json) -%>}
  }  
      
  <% if lookup_context.template_exists?("sc_grid_editView", controller._prefixes, :partial) %>
   , <%= render :partial=>"sc_grid_editView" -%>
  <% end %>
  
  
}