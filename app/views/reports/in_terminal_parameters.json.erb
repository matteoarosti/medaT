{
 "success": true,
 "items": [
  {
   xtype: 'form',
   title: '', closable: false,
   flex: 1,
	layout: {
	    type: 'vbox',
	    align : 'stretch',
	    pack  : 'start',
	}, 
     defaults: {
     	 xtype: 'textfield',
     	 margin: '10 10 0 10',
     },	
     
    items: [
       
     {xtype: 'hidden', name: 'authenticity_token', value: '<%= form_authenticity_token %>'},
     <%= raw extjs_std_combo_model('shipowner', @hh, {:allowBlank=>true}, {:multiSelect => true}) -%>,
     	
     	
										{
								            xtype: 'radiogroup',
								            fieldLabel: 'Pieni/Vuoti',
								            layout: 'hbox',
								            items: [{
								                boxLabel: 'Pieni',
								                name: 'fl_FE',
								                inputValue: 'F',
								                checked: true,
								                width: 90                
								            }, {
								                boxLabel: 'Vuoti',
								                name: 'fl_FE',
								                inputValue: 'E',
								                width: 90
								            }, {
								                boxLabel: 'Entrambi',
								                name: 'fl_FE',
								                inputValue: '',
								                width: 90
								            }]
								        }
								        
								        
								        , {
								            xtype: 'radiogroup',
								            fieldLabel: 'Ordina per',
								            layout: 'hbox',
								            items: [{
								                boxLabel: 'Tipo / Entrata',
								                name: 'seq_ord',
								                inputValue: 'tipo_date',
								                checked: true,
								                width: 180               
								            }, {
								                boxLabel: 'Entrata / Tipo',
								                name: 'seq_ord',
								                inputValue: 'date_tipo',
								                width: 180
								            }]
								        }
								           
							 ],
    
   buttons: [
	{
        text: '<i class="fa fa-th-large"></i> Sintetica',
        handler: function(){
        	fp = this.up('form');
            if(fp.getForm().isValid()){
                fp.getForm().submit({
                    method: 'POST',
                    url: '<%= url_for(:controller=>'reports', :action=>'in_terminal_sint') -%>',             
                    params: {formValues: Ext.encode(fp.getValues())},
                    method: 'POST',
                    standardSubmit: true,
                    target: '_blank'
                });
            }
        }
    }, 	{
        text: '<i class="fa fa-navicon fa"></i> Analitica',
        handler: function(){
        	fp = this.up('form');
            if(fp.getForm().isValid()){
                fp.getForm().submit({
                    method: 'POST',
                    url: '<%= url_for(:controller=>'reports', :action=>'in_terminal_analytic') -%>',             
                    params: {formValues: Ext.encode(fp.getValues())},
                    method: 'POST',
                    standardSubmit: true,
                    target: '_blank'
                });
            }
        }
    }    
   ] 
    
  }
 ]
}