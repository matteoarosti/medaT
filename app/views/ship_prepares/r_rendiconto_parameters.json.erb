{
 "success": true,
 "items": [
  {
   xtype: 'form',
   title: '', closable: false,
   flex: 1,
	layout: {
	    type: 'vbox',
	    align : 'stretch',
	    pack  : 'start',
	}, 
     defaults: {
     	 xtype: 'textfield',
     	 margin: '10 10 0 10',
     },	
     
    items: [
       
     {xtype: 'hidden', name: 'authenticity_token', value: '<%= form_authenticity_token %>'},
	 <%= raw extjs_std_hiddenfield('id', @item) -%>,
								           
	],
    
   buttons: [
	{
        text: '<i class="fa fa-th-large"></i> Visualizza',
        handler: function(){
        	fp = this.up('form');
            if(fp.getForm().isValid()){
                fp.getForm().submit({
                    method: 'POST',
                    url: '<%= url_for(:controller=>'ship_prepares', :action=>'r_rendiconto_a') -%>',             
                    params: {formValues: Ext.encode(fp.getValues())},
                    method: 'POST',
                    standardSubmit: true,
                    target: '_blank'
                });
            }
        }
    }    
   ] 
    
  }
 ]
}