	   <%= raw extjs_std_datetimefield('datetime_op', @new_rec) -%>,
	   <%= raw extjs_std_combo('inspection_type_id', @new_rec, :valueField => 'id', :displayField => 'name', :allowBlank => false, :store => extjs_std_store_data(InspectionType.get_data_json)) -%>,
