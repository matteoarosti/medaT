{
    "success": true,
 "items": [
  {
   xtype: 'form',
   flex: 1,
	layout: {
	    type: 'vbox',
	    align : 'stretch',
	    pack  : 'start',
	}, 
     defaults: {
     	 xtype: 'textfield',
     	 margin: '10 10 0 10',
     },	  
     
   items: [
    {xtype: 'hiddenfield', name: 'handling_header_id', value: <%= @new_rec.handling_header_id -%>},
    {xtype: 'hiddenfield', name: 'handling_item_type', value: '<%= @new_rec.handling_item_type -%>'},  
   // {xtype: 'hiddenfield', name: 'id', value: '<%= @new_rec.id -%>'},

	   <%= raw extjs_std_datetimefield('datetime_op', @new_rec) -%>,
       <%= raw extjs_std_combo_model('ship', @new_rec, :allowBlank => true) -%>,
       <%= raw extjs_std_textfield('voyage', @new_rec, :allowBlank => true) -%>,
       <%= raw extjs_std_combo_model('carrier', @new_rec, :allowBlank => true) -%>,
       <%= raw extjs_std_textfield('driver', @new_rec, :allowBlank => true) -%>,
       <%= raw extjs_std_textfield('seal_shipowner', @new_rec, :allowBlank => true) -%>,
       <%= raw extjs_std_textfield('seal_others', @new_rec, :allowBlank => true) -%>,
       <%= raw extjs_to_be_moved('to_be_moved', @hh, @new_rec) -%>,
       
	    {
            xtype: 'fieldset',
            title: 'In testata',
            layout: 'anchor',
            items: [
            
				{
	                xtype: 'fieldcontainer',
	                combineErrors: true,
	                msgTarget : 'side',
	                layout: 'hbox',
	                anchor: '100%',
	                defaults: {xtype: 'textfield', flex: 1, hideLabel: false, labelWidth: 120, labelAlign: 'right'},
	                items: [
    					<%= raw extjs_std_numberfield('weight_imp', @hh, :allowBlank => true, :input_prefix => 'hh_') -%>,
    					<%= raw extjs_std_numberfield('temperature_imp', @hh, :allowBlank => true, :input_prefix => 'hh_') -%>,
    					<%= raw extjs_std_textfield('imo_imp', @hh, :allowBlank => true, :input_prefix => 'hh_') -%>			                
			        ]
			    }
    		]
    	},       
       
       <%= raw extjs_notes('notes', @hh, @new_rec) -%>
   ],
      
	buttons:[<%= render :partial=>'add_handling_items_buttons' -%>]
	   
  }
 ]
}