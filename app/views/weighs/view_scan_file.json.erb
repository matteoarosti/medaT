{
 "success": true,
 "items": [
  {
   xtype: 'panel',
   flex: 1,
   layout: {
	    type: 'vbox',
	    align : 'stretch',
	    pack  : 'start',
   }, 
     defaults: {
     	 xtype: 'textfield',
     	 margin: '10 10 0 10',
     },	  
   autoScroll: true,  
   items: [    	
    	    {
     			xtype: 'image',
     			src: <%= raw @item.scan_file.url(:original).to_json %>,
     			width: '100%'
    		}
	], 
	
	
	buttons: [
		{
            text: 'Chiudi',
            handler: function (btn, evt) {
            	this.up('window').close();
    		}
	     }	
	]
	
  }
 ]
}	